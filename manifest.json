{
    "manifest_version": 3,
    "name": "celtest",
    "version": "0.2",
    "description": "Acesse facilmente ferramentas de análise de web performance através do menu de contexto.",
    "homepage_url": "https://github.com/Celere-WP/celtest",
    "author": "Célere",
    "permissions": [
      "contextMenus",
      "tabs",
      "activeTab",
      "scripting"
    ],
    "host_permissions": [
      "https://www.siteliner.com/*",
      "https://inspectwp.com/en/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
