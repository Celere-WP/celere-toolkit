{
    "manifest_version": 3,
    "name": "Celeridade",
    "version": "0.1.1",
    "description": "Acesse ferramentas de análise de performance de sites através do menu de contexto.",
    "homepage_url": "https://github.com/Celere-WP/celeridade",
    "author": "Célere",
    "permissions": [
      "contextMenus",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  }
